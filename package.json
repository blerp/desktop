{
  "name": "blerp-desktop",
  "productName": "BlerpDesktop",
  "version": "0.0.5",
  "description": "My Electron application description",
  "license": "MIT",
  "keywords": [],
  "author": "Blerp",
  "scripts": {
    "dev": "electron-webpack dev",
    "compile": "electron-webpack",
    "dist": "yarn compile && electron-builder",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null",
    "build-all": "yarn compile && electron-builder build -mwl --publish never",
    "build-windows": "yarn compile && electron-builder build --win --publish never",
    "build-mac": "yarn compile && electron-builder build --mac --publish never",
    "build-linux": "yarn compile && electron-builder build --linux --publish never",
    "deploy-all": "yarn compile && electron-builder build -mwl --publish always",
    "deploy-windows": "yarn compile && electron-builder build --win --publish always",
    "deploy-mac": "yarn compile && electron-builder build --mac --publish always",
    "deploy-linux": "yarn compile && electron-builder build --linux --publish always",
    "postinstall": "electron-builder install-app-deps"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/blerp/desktop.git"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.13.9",
    "@babel/preset-react": "^7.12.13",
    "electron": "8.2.0",
    "electron-builder": "^22.4.1",
    "electron-webpack": "^2.8.2",
    "webpack": "~4.42.1"
  },
  "dependencies": {
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "css-loader": "^5.1.1",
    "electron-is-dev": "^2.0.0",
    "electron-updater": "^4.3.8",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "source-map-support": "^0.5.19"
  }
}
